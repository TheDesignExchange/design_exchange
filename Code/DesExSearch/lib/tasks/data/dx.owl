    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#WhoDo -->

    <owl:Class rdf:about="&DesignExchange_Methods;WhoDo">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Documents"/>
