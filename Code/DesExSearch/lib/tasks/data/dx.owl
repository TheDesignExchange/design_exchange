    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Action_Planning -->

    <owl:Class rdf:about="&DesignExchange_Methods;Action_Planning">
        <rdfs:label>Action Planning</rdfs:label>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Documents"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;forAudience"/>
