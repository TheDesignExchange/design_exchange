    <owl:Class rdf:about="&DesignExchange_Methods;Behavior_Sampling">
        <rdfs:label>Behavior Sampling</rdfs:label>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Sampling-based"/>
