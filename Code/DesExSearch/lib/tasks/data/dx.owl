    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Diagnostic_Evaluation -->

    <owl:Class rdf:about="&DesignExchange_Methods;Diagnostic_Evaluation">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Evaluative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;hasSpace"/>
