    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Autoethnography -->

    <owl:Class rdf:about="&DesignExchange_Methods;Autoethnography">
        <rdfs:label>Autoethnography</rdfs:label>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Ethnography"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;hasParticipant"/>
