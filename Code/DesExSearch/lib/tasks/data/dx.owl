    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/untitled-ontology-4#Action_Learning -->

    <owl:Class rdf:about="http://www.semanticweb.org/howard/ontologies/2014/0/untitled-ontology-4#Action_Learning">
        <rdfs:label>Action Learning</rdfs:label>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Autoethnography"/>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Formatted_Conversations"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;hasParticipant"/>
                <owl:someValuesFrom rdf:resource="&DesignExchange_Methods;Creative"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
