    </owl:Class>
    


    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Usability_Testing -->

    <owl:Class rdf:about="&DesignExchange_Methods;Usability_Testing">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Evaluative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;hasResearcher"/>
