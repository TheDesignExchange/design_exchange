        <isMethodCategory></isMethodCategory>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Visual_Agenda -->

    <owl:Class rdf:about="&DesignExchange_Methods;Visual_Agenda">
