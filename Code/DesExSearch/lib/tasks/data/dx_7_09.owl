    <owl:Class rdf:about="&DesignExchange_Methods;Try_It_Yourself">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Evaluative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
