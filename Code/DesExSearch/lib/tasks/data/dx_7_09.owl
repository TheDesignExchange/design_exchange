    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Web_Analytics -->

    <owl:Class rdf:about="&DesignExchange_Methods;Web_Analytics">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Capturing_Organized_Data"/>
